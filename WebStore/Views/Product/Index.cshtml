@model List<WebStore.Models.Product>

<!DOCTYPE html>
<html>
<head>
    <title>Список продуктов</title>
</head>
<body>
    <h1>Список продуктов</h1>
    
    <table>
        <tr>
            <th>Наименование</th>
            <th>Цена</th>
        </tr>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.Name</td>
                <td>@product.Price</td>
            </tr>
        }
    </table>
</body>
</html>
